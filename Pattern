/*
	A A A A A 
	B B B B 
	C C C 
	D D 
	E
*/
class Question4
{
	public static void test(int n) {
		char a ='A';
		for(int i=n;i>=1;i--)
		{
			for(int j=1;j<=i;j++)
			{
				System.out.print(a+" ");
			}
			a++;
			System.out.println();
		}
	}
	public static void main(String[] args) {
		System.out.println("Program Starts...");
		test(5);
		System.out.println("Program Ends");
	}
}
